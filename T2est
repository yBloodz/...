local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "yBloodz premium", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest", IntroEnabled = true, IntroText = "yBloodz"})

local GkTab = Window:MakeTab({
	Name = "Goalkeeper",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

